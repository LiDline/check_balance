{
  "name": "my-project",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.1.2",

  "scripts": {
    "dev:backend": "dotenv -- pnpm --filter ./backend dev",
    "dev": "concurrently \"pnpm --filter frontend dev\" \"dotenv -- pnpm --filter ./backend dev\"",
    "build": "pnpm --filter frontend build && pnpm --filter backend build",
    "start": "pnpm --filter backend start"
  },
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2"
  }
}
